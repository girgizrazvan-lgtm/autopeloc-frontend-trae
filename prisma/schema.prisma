generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas   = ["auth", "public"]
}

model User {
  id            String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  auth0_id      String?
  email         String
  name          String?
  role          String?   @default("user")
  company       String?
  status        String?   @default("pending")
  password_hash String?
  org_id        String?   @db.Uuid
  created_at    DateTime? @default(now()) @db.Timestamptz(6)
  updated_at    DateTime? @default(now()) @db.Timestamptz(6)
  sessions      Session[]

  @@index([email])
  @@index([role])
  @@index([status])
  @@map("users")
  @@schema("public")
}

model Session {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user_id    String    @db.Uuid
  token      String
  expires_at DateTime  @db.Timestamptz(6)
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  users      User      @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([token])
  @@index([user_id])
  @@map("sessions")
  @@schema("public")
}

model BlogPost {
  id           String    @id @default(dbgenerated("(gen_random_uuid())::text"))
  slug         String    @unique
  title        String
  description  String
  content      String
  excerpt      String?
  category     String
  read_time    String?
  keywords     String?
  published_at DateTime? @default(now()) @db.Timestamptz(6)
  updated_at   DateTime? @default(now()) @db.Timestamptz(6)
  is_published Boolean?  @default(true)
  og_image     String?
  author       String?   @default("autopeloc.ro")

  @@index([is_published, published_at(sort: Desc)], map: "idx_blog_posts_published")
  @@index([slug], map: "idx_blog_posts_slug")
  @@map("blog_posts")
  @@schema("public")
}

model FAQ {
  id        String   @id @default(dbgenerated("(gen_random_uuid())::text"))
  question  String
  answer    String
  order     Int?     @default(0)
  is_active Boolean? @default(true)

  @@index([is_active], map: "idx_faqs_active")
  @@index([order], map: "idx_faqs_order")
  @@map("faqs")
  @@schema("public")
}

model Vehicle {
  id           String   @id @default(dbgenerated("(gen_random_uuid())::text"))
  name         String
  brand        String
  model        String
  year         Int?
  image        String?
  sipp_code    String
  category     String
  acriss_code  String?
  engine       String?
  transmission String?
  seats        Int?
  fuel         String?
  order        Int?     @default(0)
  is_active    Boolean? @default(true)

  @@index([is_active], map: "idx_vehicles_active")
  @@index([category], map: "idx_vehicles_category")
  @@index([order], map: "idx_vehicles_order")
  @@map("vehicles")
  @@schema("public")
}

model AboutPage {
  id      String  @id @default(dbgenerated("(gen_random_uuid())::text"))
  section String  @unique
  title   String?
  content String
  order   Int?    @default(0)

  @@index([section], map: "idx_about_pages_section")
  @@map("about_pages")
  @@schema("public")
}

/// Supabase live visitors (used by Admin â†’ Vizitatori Live)
model LiveVisitor {
  id            String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  session_id    String
  current_page  String
  page_title    String?
  referrer      String?
  user_agent    String?
  first_seen    DateTime @default(now()) @db.Timestamptz(6)
  last_activity DateTime @default(now()) @db.Timestamptz(6)
  country_code  String?
  country       String?
  region        String?
  city          String?
  latitude      Float?
  longitude     Float?
  is_bot        Boolean  @default(false)
  session_start DateTime @default(now()) @db.Timestamptz(6)

  @@map("live_visitors")
  @@schema("public")
  @@unique([session_id], name: "live_visitors_session_id_idx")
}

/// Supabase reservations (used by booking and admin)
model Reservation {
  id                 String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  session_id         String?
  source             String?   @default("website")
  contact_name       String?
  contact_phone      String?
  contact_email      String?
  final_price        Decimal?  @db.Decimal
  currency           String?   @default("RON")
  document_url       String?
  document_blob_id   String?
  status             String?   @default("pending")
  notes              String?
  created_at         DateTime? @default(now()) @db.Timestamptz(6)
  updated_at         DateTime? @default(now()) @db.Timestamptz(6)
  city               String?
  reservation_number String?   @unique(map: "uniq_reservations_reservation_number")
  created_by         String?   @db.Uuid
  users              users?    @relation(fields: [created_by], references: [id], onUpdate: NoAction)

  @@index([created_by], map: "idx_reservations_created_by")
  @@index([created_at])
  @@index([session_id])
  @@index([status])
  @@map("reservations")
  @@schema("public")
}

/// Booking funnel step tracking (used in create-reservation route)
model BookingFunnelStep {
  id               String           @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  session_id       String
  step_number      Int
  step_name        String
  step_data        Json?
  completed_at     DateTime?        @default(now()) @db.Timestamptz(6)
  booking_sessions booking_sessions @relation(fields: [session_id], references: [session_id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([session_id, step_number])
  @@index([completed_at])
  @@index([session_id])
  @@index([step_number])
  @@map("booking_funnel_steps")
  @@schema("public")
}

